/**index.wxss**/
.test {
  margin: 20px;
  width: 350rpx;
  height: 300rpx;
  border: solid 1rpx;
  border-radius: 12px;
  box-shadow: 0 0 1em red;
}

.banner {
  width: 750rpx;
  height: 560rpx;
}

.page-body {
  display: flex;
  flex-flow: column;
}

.nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 700rpx;
  height: 150rpx;
}

.nav view {
  font-size: 30rpx;
  width: 150rpx;
  height: 70rpx;
  background: gray;
  line-height: 70rpx;
  text-align: center;
  border-radius: 35rpx;
}
